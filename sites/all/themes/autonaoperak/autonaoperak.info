name = Autonaoperak
description = Theme based on Bootstrap, developed by <a href="https://proficio.cz">PROFICIO</a>
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[help]           = 'Help'
regions[topbar]         = 'Topbar'
regions[navigation]     = 'Navigation'
regions[content]        = 'Content'
regions[content_above]  = 'Content - above'
regions[content_below]  = 'Content - below'
regions[sidebar_first]  = 'Sidebar first'
regions[sidebar_second] = 'Sidebar second'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'


;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;

stylesheets[all][] = css/style.css
stylesheets[print][] = css/print.css


;;;;;;;;;;;;;;;;;;;;;
;; Scripts
;;;;;;;;;;;;;;;;;;;;;

scripts[] = 'plugins/bootstrap/js/affix.js'
scripts[] = 'plugins/bootstrap/js/alert.js'
scripts[] = 'plugins/bootstrap/js/button.js'
scripts[] = 'plugins/bootstrap/js/carousel.js'
scripts[] = 'plugins/bootstrap/js/collapse.js'
scripts[] = 'plugins/bootstrap/js/dropdown.js'
scripts[] = 'plugins/bootstrap/js/modal.js'
scripts[] = 'plugins/bootstrap/js/tooltip.js'
scripts[] = 'plugins/bootstrap/js/popover.js'
scripts[] = 'plugins/bootstrap/js/scrollspy.js'
scripts[] = 'plugins/bootstrap/js/tab.js'
scripts[] = 'plugins/bootstrap/js/transition.js'
scripts[] = 'plugins/owlcarousel/dist/owl.carousel.min.js'
scripts[] = 'plugins/jquery-viewport-checker/dist/jquery.viewportchecker.min.js'
scripts[] = 'plugins/bootstrap-slider/dist/bootstrap-slider.min.js'
scripts[] = 'plugins/countUp.js/dist/countUp.min.js'
scripts[] = 'plugins/jquery.inview/jquery.inview.min.js'
scripts[] = 'plugins/accounting.js/accounting.min.js'
scripts[] = 'plugins/js-cookie/src/js.cookie.js'
scripts[] = 'js/eucookie.js'
scripts[] = 'js/vat.js'
scripts[] = 'js/script.js'

; Disable BootstrapCDN so the compiled LESS source files can be used.
settings[bootstrap_cdn] = ''
